@page
@model APPClinet.Areas.Identity.Pages.Account.Dashboard.User.Settings.AdminSettingsModel
@using APPClinet.Areas.Identity.Data.Interfaces
@inject IApplicationUser _iuser
@{
    var faSettings = APPClinet.Messages.faMessage.Settings.ToString();
    ViewData["Title"] = faSettings;
    Layout = "~/Areas/Identity/Pages/Account/Dashboard/User/_UserNavrtl.cshtml";
    var faWebsite = APPClinet.Messages.faMessage.Website.ToString();
    var faNewsletter = APPClinet.Messages.faMessage.Newsletter.ToString();
}

<div class="p-0 mb-2">
    <div class="row">
        <div class="col-sm-12">
            <h2 class="text-left">@faSettings&nbsp;@faWebsite</h2>
        </div>
    </div>
</div>
<div class="dropdown-divider"></div>
<div class="row">
    <div class="col-12">
        <!-- Accordions Start -->
        <div class="col mt-4 pt-2" id="accordions">
            <div class="component-wrapper rounded shadow">
                <div class="p-4 border-bottom">
                    <h4 class="title mb-0"><i class="fas fa-cogs mr-2"></i>@faSettings</h4>
                </div>
                <div class="p-4">
                    <div class="faq-content">
                        <div class="accordion" id="accordionExample">
                            <div class="card border-0 rounded mb-2">
                                <a data-toggle="collapse" href="#ollapseone" class="faq position-relative collapsed" aria-expanded="false" aria-controls="ollapseone">
                                    <div class="card-header border-0 bg-light p-3 pr-5" id="headingone">
                                        <h6 class="title mb-0">@*<span class="text-danger font-weight-bold">@faCategoriesForExample</span>&nbsp;*@<span class="text-primary font-weight-bold">@faSettings&nbsp;@faWebsite</span></h6>
                                    </div>
                                </a>
                                <div id="ollapseone" class="collapse" aria-labelledby="headingone" data-parent="#accordionExample">
                                    <div class="card-body px-2 py-4">
                                        <a class="dropdown-item text-warning font-weight-bold" href="/Identity/Account/Dashboard/User/Settings/Newsletter"><i class="fas fa-edit mr-2"></i>@faNewsletter</a>
                                        @*<p class="text-muted mb-0 faq-ans"></p>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--end col-->
        <!-- Accordions End -->
    </div>
</div>