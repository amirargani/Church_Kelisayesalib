@page
@model APPClinet.Areas.Identity.Pages.Account.Dashboard.Admin.Settings.SocialNetworksModel
@{
    var faDetailEdit = APPClinet.Messages.faMessage.DetailEdit.ToString();
    ViewData["Title"] = faDetailEdit;
    Layout = "~/Areas/Identity/Pages/Account/Dashboard/Admin/_UserNavrtl.cshtml";
    var faMenusBack = APPClinet.Messages.faMessage.MenusBack.ToString();
    var faSettings = APPClinet.Messages.faMessage.Settings.ToString();
    var faEmail = APPClinet.Messages.faMessage.Email.ToString();
    var faPhoneNumber = APPClinet.Messages.faMessage.PhoneNumber.ToString();
    var faPrivateNumber = APPClinet.Messages.faMessage.PrivateNumber.ToString();
    var faTelegram = APPClinet.Messages.faMessage.Telegram.ToString();
    var faYoutube = APPClinet.Messages.faMessage.Youtube.ToString();
    var faInstagram = APPClinet.Messages.faMessage.Instagram.ToString();
    var faTwitter = APPClinet.Messages.faMessage.Twitter.ToString();
    var faFacebook = APPClinet.Messages.faMessage.Facebook.ToString();
    var faEmbedLinkGoogleMap = APPClinet.Messages.faMessage.EmbedLinkGoogleMap.ToString();
    var faButtonSave = APPClinet.Messages.faMessage.ButtonSave.ToString();
}

<div class="p-0 mb-2">
    <div class="row">
        <div class="col-sm-5">
            <h2 class="text-left">@faDetailEdit</h2>
        </div>
        <div class="col-sm-7">
            <a class="btn btn-dark font-weight-bold" href="/Identity/Account/Dashboard/Admin/Settings/Index"><i class="fas fa-arrow-alt-circle-left mr-2"></i>@faSettings</a>
            <a class="btn btn-secondary font-weight-bold" href="/Identity/Account/Dashboard/Admin/Menus"><i class="fas fa-th-list mr-2"></i>@faMenusBack</a>
        </div>
    </div>
</div>
<div class="dropdown-divider"></div>
@*<partial name="_StatusMessage" model="Model.StatusMessage" />*@
<form class="socialnetworks-form mt-4" id="socialnetworks-form" method="post">
    <div class="row">

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.Email">@faEmail</label>
                <i class="fas fa-envelope icon-sm icons"></i>
                <input type="email" asp-for="Input.Email" class="form-control pl-5 fen" placeholder="">
                <span asp-validation-for="Input.Email" class="text-danger"></span>
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <strong class="text-danger f15 fen" style="float: left;">+49 16X XXXXXXX | +49 17X XXXXXXXX</strong><br />
            <div class="form-group position-relative">
                <label asp-for="Input.PhoneNumber">@faPhoneNumber</label>
                <i class="fas fa-mobile icon-sm icons"></i>
                <input type="tel" asp-for="Input.PhoneNumber" class="form-control pl-5 fen" placeholder="">
                <span asp-validation-for="Input.PhoneNumber" class="text-danger"></span>
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <strong class="text-danger f15 fen" style="float: left;">+49 4XX XXXXXXX</strong><br />
            <div class="form-group position-relative">
                <label asp-for="Input.PrivateNumber">@faPrivateNumber</label>
                <i class="fas fa-phone icon-sm icons"></i>
                <input type="tel" asp-for="Input.PrivateNumber" class="form-control pl-5 fen" placeholder="">
                <span asp-validation-for="Input.PrivateNumber" class="text-danger"></span>
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.Telegram">@faTelegram</label>
                <i class="fab fa-telegram icon-sm icons"></i>
                <input type="text" asp-for="Input.Telegram" class="form-control pl-5 fen" placeholder="">
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.Youtube">@faYoutube</label>
                <i class="fab fa-youtube icon-sm icons"></i>
                <input type="text" asp-for="Input.Youtube" class="form-control pl-5 fen" placeholder="">
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.Instagram">@faInstagram</label>
                <i class="fab fa-instagram-square icon-sm icons"></i>
                <input type="text" asp-for="Input.Instagram" class="form-control pl-5 fen" placeholder="">
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.Twitter">@faTwitter</label>
                <i class="fab fa-twitter icon-sm icons"></i>
                <input type="text" asp-for="Input.Twitter" class="form-control pl-5 fen" placeholder="">
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.Facebook">@faFacebook</label>
                <i class="fab fa-facebook-square icon-sm icons"></i>
                <input type="text" asp-for="Input.Facebook" class="form-control pl-5 fen" placeholder="">
            </div>
        </div><!--end col-->

        <div class="col-lg-12">
            <div class="form-group position-relative">
                <label asp-for="Input.EmbedLinkGoogleMap">@faEmbedLinkGoogleMap</label>
                <i class="fas fa-map icon-sm icons"></i>
                <input type="text" asp-for="Input.EmbedLinkGoogleMap" class="form-control pl-5 fen" placeholder="">
            </div>
        </div><!--end col-->

        <div class="col-lg-12 mb-0">
            <button class="btn btn-primary btn-block">@faButtonSave</button>
        </div><!--end col-->
    </div><!--end row-->
</form>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}